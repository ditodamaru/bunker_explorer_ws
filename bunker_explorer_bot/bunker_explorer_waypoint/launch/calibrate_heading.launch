<?xml version="1.0"?>

<launch>

  <group ns="/bunker_explorer_waypoint">
  <!-- Load current navsat parameters -->
  <rosparam command="load" file="$(find bunker_explorer_waypoint)/params/navsat_params.yaml" />

  <!-- Start calibration node -->
  <node name="calibrate_heading" pkg="bunker_explorer_waypoint" type="calibrate_heading" output="screen" />
    <!-- set x linear velocity (m/s) -->
    <param name="/bunker_explorer_waypoint/x_vel" value="0.5" />

    <!-- set how long to publish velocity command (s) -->
    <param name="/bunker_explorer_waypoint/x_vel_time" value="10" />
  </group>

</launch>
